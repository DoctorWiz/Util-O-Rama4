		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows10.0.19041.0</TargetFramework>
		<RootNamespace>UtilORama4</RootNamespace>
		<AssemblyName>Map-O-Rama</AssemblyName>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
		<PlatformTarget>x64</PlatformTarget>
		<RepositoryUrl>https://github.com/DoctorWiz/Util-O-Rama4/tree/master/MapORama</RepositoryUrl>
		<PublishUrl>ftp://wizlights.com/utilorama/download/</PublishUrl>
		<InstallUrl>http://wizlights.com/utilorama/download/</InstallUrl>
		<SupportUrl>http://wizlights.com/utilorama/maporama/default.html</SupportUrl>
		<ProductName>Map-O-Rama</ProductName>
		<PublisherName>Wâš¡zlights Software</PublisherName>
		<SuiteName>Util-O-Rama</SuiteName>
		<CreateWebPageOnPublish>true</CreateWebPageOnPublish>
		<WebPage>http://wizlights.com/utilorama/maporama/default.html</WebPage>
		<ApplicationRevision>0</ApplicationRevision>
		<ApplicationVersion>4.0.0.%2a</ApplicationVersion>
		<PackageTags>Util-O-Rama; Light-O-Rama; xLights; Christmas; Channels; Mapping; Light Show</PackageTags>
		<Description>
			Map channels from one Light-O-Rama Showtime S4 sequence to another sequence with a different channel layout.
		</Description>
		<ApplicationIcon>Map-O-Rama.ico</ApplicationIcon>
		<IsPublishable>False</IsPublishable>
		<PackageProjectUrl>http://wizlights/utilorama/maporama</PackageProjectUrl>
		<PackageIcon>MapORamaBulb.png</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
